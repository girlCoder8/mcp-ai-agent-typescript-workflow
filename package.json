{
  "name": "mcp-ai-agent-typescript-workflow",
  "version": "1.0.0",
  "description": "CI/CD Automation Pipeline with MCP Agent Orchestrator",
  "main": "dist/index.js",
  "scripts": {
    "// === Build Script ===": "",
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "// === Clean ALL Report Files Script ===": "",
    "clean": "rm -rf dist/ test-results/ allure-report/ allure-results/ pipeline-reports/ playwright-report/ test-reports/ test-case-results/ logs/",
    "// === Allure Report Scripts ===": "",
    "allure:generate": "npx allure generate allure-results -o allure-report || echo 'allure results generating report from.'",
    "allure:open": "npx allure open allure-report",
    "// === WDIO Mobile Simulator and Real Device Scripts ===": "",
    "test:wdio:ai:mobile": "wdio run wdio.conf.ai.sim.ts",
    "test:wdio:ai:simulator": "npx wdio run src/config/wdio-mobile/wdio.ios.conf.ai.ts",
    "// === Headspin Mobile Cloud Scripts ===": "",
    "test:headspin": "HEADSPIN_ENABLED=true HEADSPIN_DEVICE_CLOUD=true wdio run wdio.conf.ai.sim.ts",
    "test:headspin:debug": "HEADSPIN_ENABLED=true HEADSPIN_DEVICE_CLOUD=true DEBUG=true wdio run wdio.conf.ai.sim.ts",
    "test:headspin:ci": "HEADSPIN_ENABLED=true HEADSPIN_DEVICE_CLOUD=true CI=true wdio run wdio.conf.ai.sim.ts",
    "test:headspin:performance": "HEADSPIN_ENABLED=true HEADSPIN_DEVICE_CLOUD=true HEADSPIN_CAPTURE_PERFORMANCE=true wdio run wdio.conf.ai.sim.ts",
    "// === Playwright Web and API Scripts ===": "",
    "dev": "npx playwright test --config=playwright.config.ai.ts tests/web tests/api",
    "test:ai:web": "npx playwright test --config=playwright.config.ai.ts tests/web/",
    "test:ai:api": "npx playwright test --config=playwright.config.ai.ts tests/api/",
    "test:ai:all": "npx playwright test --config=playwright.config.ai.ts tests/web/ tests/api/",
    "test:ai:headed": "npx playwright test --config=playwright.config.ai.ts --headed",

    "// =====================================================================": "",
    "// ======== Test Case Setup, Validate and Report =======================": "",
    "// === Test Case Setup and Validation ===": "",
    "setup:files": "npx ts-node scripts/testData/setup-test-data.ts",
    "data:validate": "npx ts-node scripts/validateTestData/validate-test-data.ts",
    "health:check": "npx ts-node scripts/healthCheck/health-check.ts",
    "// === Test Generation Script ===": "",
    "generate:tests": "npm run setup:files && npm run playwright:install",
    "playwright:tests": "npx playwright test --config=playwright.config.ai.ts tests/web tests/api",
    "// === Reporting ===": "",
    "report:summary": "npx ts-node scripts/testSummary/test-summary.ts",
    "report:show": "playwright show-report test-reports/playwright-report",
    "report:open": "open test-reports/test-case-report.html",
    "report:json": "cat test-reports/results.json | jq .",
    "// ========================================================================": "",

    "// === Test Execution ===": "",
    "test:non-headed": "npx playwright test --config=playwright.config.ai.ts tests/web tests/api",
    "test:headed": "npx playwright test --headed --config=playwright.config.ai.ts tests/web",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "// === Generated Tests ===": "",
    "test:generated": "playwright test tests/ai-generated/",
    "test:generated:web": "playwright test tests/ai-generated/web/",
    "test:generated:mobile": "playwright test tests/ai-generated/mobile/",
    "// === Setup and Installation ===": "",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "// === Manual Tests ===": "",
    "test:manual": "playwright test --project=manual-web-tests",
    "test:api": "playwright test --project=api-tests",
    "test:visual": "playwright test --project=visual-tests",
    "test:performance": "playwright test --project=performance-tests",
    "// === Test Filtering ===": "",
    "test:smoke": "playwright test --grep @smoke",
    "test:regression": "playwright test --grep @regression",
    "test:critical": "playwright test --grep @critical",
    "test:auth": "playwright test --grep @auth",
    "test:search": "playwright test --grep @search",
    "// === Comprehensive Test Suites ===": "",
    "test:full": "npm run clean && npm run generate:tests && npm run test",
    "test:quick": "playwright test --project=chromium-web tests/generated/web/",
    "test:nightly": "npm run test:full && npm run test:visual && npm run test:performance",
    "// === CI/CD Scripts ===": "",
    "test:ci": "npm run generate:tests && playwright test --reporter=github",
    "test:ci:web": "playwright test --project=chromium-web --reporter=github",
    "test:ci:mobile": "playwright test --project=mobile-chrome --reporter=github",
    "test:parallel": "playwright test --workers=4",
    "test:serial": "playwright test --workers=1",
    "// === Wiremock Scripts ===": "",
    "wiremock:start": "./scripts/start-wiremock.sh",
    "wiremock:stop": "./scripts/stop-wiremock.sh",
    "// === Health Checks ===": "",
    "health:checks": "npx ts-node scripts/health-check.ts",
    "health:urls": "npx ts-node scripts/check-urls.ts",
    "health:dependencies": "npm audit && npm outdated"
  },
  "keywords": [],
  "author": "Barb Gordon",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/chai": "^5.2.2",
    "@types/jquery": "^3.5.32",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.4",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/uuid": "^10.0.0",
    "@types/webdriverio": "^4.13.3",
    "@wdio/allure-reporter": "^9.18.0",
    "@wdio/appium-service": "^9.18.1",
    "@wdio/cli": "^9.18.4",
    "@wdio/json-reporter": "^9.18.0",
    "@wdio/junit-reporter": "^9.18.0",
    "@wdio/local-runner": "^9.18.1",
    "@wdio/mocha-framework": "^9.18.0",
    "@wdio/spec-reporter": "^9.18.0",
    "@wdio/types": "^9.16.2",
    "allure-playwright": "^3.3.2",
    "appium-xcuitest-driver": "^9.9.6",
    "cross-env": "^10.0.0",
    "expect-webdriverio": "^5.4.0",
    "http-server": "^14.1.1",
    "i": "^0.3.7",
    "jq": "^1.7.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "webdriverio": "^9.18.1",
    "wiremock": "^3.13.1"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "chai": "^5.2.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "execa": "^9.6.0",
    "openai": "^5.10.2",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "uuid": "^11.1.0"
  }
}
